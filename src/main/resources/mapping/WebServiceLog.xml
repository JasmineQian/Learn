<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.mapper.WebserviceLogMapper">


    <insert id="insertSelective" parameterType="com.example.demo.bean.WebserviceLog">
        insert swebservicelog(sessionid,remoteipaddr,requesturl,start_dt,end_dt,clienthost,useragent,method,params,paramsvalue,return_msg,exception,creation_dt,update_dt,creationuid,
        updateuid,name,return_code,return_message,source)
        values(#{sessionid},#{remoteipaddr},#{requesturl},#{start_dt},#{end_dt},#{clienthost},#{useragent},#{method},#{params},#{paramsvalue},#{return_msg},#{exception},#{creation_dt},#{update_dt},
        #{creationuid},#{updateuid},#{name},#{return_code},#{return_message},#{source})
    </insert>

</mapper>
